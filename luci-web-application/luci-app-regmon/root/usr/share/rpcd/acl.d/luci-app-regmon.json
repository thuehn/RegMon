{
	"luci-app-regmon": {
		"description": "Grant access to RegMon configuration",
		"read": {
			"cgi-io": [ "list", "read" ],
			"file": {
                "/sys/kernel/debug/ieee80211/phy0/regmon": [ "read" ]
			},
			"uci": [ "regmon" ]
		},
		"write": {
			"uci": [ "regmon" ],
            "file": {
                "/tmp/regmon": [ "write" ],
                "/etc/collectd.conf": [ "write" ]
                }
		}
	}
}
